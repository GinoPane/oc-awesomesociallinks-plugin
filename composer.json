{
  "name": "ginopane/awesomesociallinks",
  "type": "october-plugin",
  "description": "List of social icons made easy",
  "homepage": "https://github.com/GinoPane/oc-awesomesociallinks-plugin",
  "keywords": ["october", "cms", "icons", "links", "customization", "plugin", "gino pane"],
  "authors": [
    {
      "name": "Siarhei <Gino Pane> Karavai",
      "homepage": "http://sergey.karavay.name",
      "role": "Developer"
    }
  ],
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpmd/phpmd" : "^2.6"
  },
  "scripts": {
    "psr2check": [
      "@php vendor/bin/phpcs --standard=PSR2 src/"
    ],
    "psr2autofix": [
      "@php vendor/bin/phpcbf --standard=PSR2 src/"
    ]
  }
}
